<h1>Accounts</h1>
 
	<table>
	<tr>
		<th>Name</th>
		<th>Email</th>
		<th></th>
		<th></th>
	</tr>

	<% @accounts.each do |account| %>
	<tr>
		<td><%= account.id %></td>
		<td><%= account.name %></td>
		<td><%= account.email %></td>
		<td><%= link_to 'Show', account %></td>
		<td><%= link_to 'Edit', edit_account_path(account) %></td>
	</tr>
	<% end %>
</table>
